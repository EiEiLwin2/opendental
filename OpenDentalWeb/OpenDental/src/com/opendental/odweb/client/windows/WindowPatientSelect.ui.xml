<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
	xmlns:ui="urn:ui:com.google.gwt.uibinder" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:od="urn:import:com.opendental.odweb.client.ui">

	  <g:DockPanel ui:field="panelContainer">
	    <g:Dock direction="CENTER">
	  		<od:ODGrid ui:field="GridMain"/>
		</g:Dock>
		<g:Dock direction="EAST">
		  <g:VerticalPanel>
		    <g:Label>Search by fields go here:</g:Label>
		    <g:Button ui:field="butSearch" text="Search" />
			<g:HorizontalPanel>
				<g:Button ui:field="butOK" text="OK" />
				<g:Button ui:field="butCancel" text="Cancel" />
			</g:HorizontalPanel>
		  </g:VerticalPanel>
		</g:Dock>
	  </g:DockPanel>
<!-- 	</g:HTMLPanel> -->
</ui:UiBinder>