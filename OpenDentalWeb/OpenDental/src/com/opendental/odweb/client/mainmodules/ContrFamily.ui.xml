<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
	xmlns:ui="urn:ui:com.google.gwt.uibinder" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:odgrid="urn:import:com.opendental.odweb.client.ui">
	
	<g:SimplePanel ui:field="panelContainer">
		<g:DockPanel>
			<g:Dock direction="NORTH">
				<g:VerticalPanel>
					<g:cell horizontalAlignment="ALIGN_CENTER">
						<g:Label>ODToolBar goes here.</g:Label>
					</g:cell>
					<g:HorizontalPanel>
						<g:SimplePanel ui:field="panelPicture" width="100" height="100"><g:Label>Patient Picture Unavailable</g:Label></g:SimplePanel>
						<odgrid:ODGrid ui:field="gridFamily"/>
						<odgrid:ODGrid ui:field="gridRecall"/>
					</g:HorizontalPanel>
				</g:VerticalPanel>
			</g:Dock>
			<g:Dock direction="CENTER">
				<g:HorizontalPanel>
					<odgrid:ODGrid ui:field="gridPat"/>
					<odgrid:ODGrid ui:field="gridSuperFam"/>
					<odgrid:ODGrid ui:field="gridIns"/>
				</g:HorizontalPanel>
			</g:Dock>
		</g:DockPanel>
	</g:SimplePanel>
	
</ui:UiBinder>