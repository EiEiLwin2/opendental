<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
	xmlns:ui="urn:ui:com.google.gwt.uibinder" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:odgrid="urn:import:com.opendental.odweb.client.ui">
	
	<g:SimplePanel ui:field="panelContainer">
		<g:DockPanel>
			<g:Dock direction="NORTH">
				<!-- OD tool bar goes here. -->
				<g:SimplePanel></g:SimplePanel>
			</g:Dock>
			<g:Dock direction="CENTER">
				<odgrid:ODGrid ui:field="gridMain"/>
			</g:Dock>
		</g:DockPanel>
	</g:SimplePanel>
	
</ui:UiBinder>