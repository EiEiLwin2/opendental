<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
	xmlns:ui="urn:ui:com.google.gwt.uibinder" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>	
	</ui:style>
	
	<g:VerticalPanel ui:field="panelContainer">
		<g:Label>User</g:Label>
		<g:TextBox ui:field="textUser" />
		<g:Label>Password</g:Label>
		<g:TextBox ui:field="textPassword" />
		<g:cell horizontalAlignment="ALIGN_RIGHT" width="100%">
			<g:Button ui:field="butOK" text="OK" />
		</g:cell>
	</g:VerticalPanel>
	
</ui:UiBinder>
