<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:ser="http://schemas.microsoft.com/2003/10/Serialization/" xmlns:tns="http://schemas.datacontract.org/2004/07/DivvyConnect.Services" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/DivvyConnect.Services" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="http://www.divvysystems.com/DivvyConnectAPI/DivvyConnect.Services.PostcardService.svc?xsd=xsd1" namespace="http://schemas.microsoft.com/2003/10/Serialization/" />
  <xs:complexType name="ArrayOfPostcard">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Postcard" nillable="true" type="tns:Postcard" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfPostcard" nillable="true" type="tns:ArrayOfPostcard" />
  <xs:complexType name="Postcard">
    <xs:sequence>
      <xs:element minOccurs="0" name="AppointmentDateTime" type="xs:dateTime" />
      <xs:element minOccurs="0" name="DesignID" type="xs:int" />
      <xs:element minOccurs="0" name="ExternalPostcardID" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="Message" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="Recipient" nillable="true" type="tns:Recipient" />
    </xs:sequence>
    <xs:attribute ref="ser:Id" />
    <xs:attribute ref="ser:Ref" />
  </xs:complexType>
  <xs:element name="Postcard" nillable="true" type="tns:Postcard" />
  <xs:complexType name="Recipient">
    <xs:sequence>
      <xs:element minOccurs="0" name="Address1" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="Address2" nillable="true" type="xs:string" />
