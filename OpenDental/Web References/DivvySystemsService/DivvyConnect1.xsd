    <xs:restriction base="xs:string">
      <xs:enumeration value="Successful" />
      <xs:enumeration value="CardNotLoaded" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="PostcardCode" nillable="true" type="tns:PostcardCode" />
