    </xs:sequence>
  </xs:complexType>
  <xs:element name="PostcardReturnMessage" nillable="true" type="tns:PostcardReturnMessage" />
  <xs:simpleType name="MessageCode">
    <xs:restriction base="xs:string">
      <xs:enumeration value="CompletedSucessfully" />
      <xs:enumeration value="CompletedWithErrors" />
      <xs:enumeration value="Failure" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="MessageCode" nillable="true" type="tns:MessageCode" />
  <xs:complexType name="ArrayOfSinglePostcardMessage">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="SinglePostcardMessage" nillable="true" type="tns:SinglePostcardMessage" />
